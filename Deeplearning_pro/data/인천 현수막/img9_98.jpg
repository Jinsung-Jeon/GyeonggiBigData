	function selectMainTab(obj) {
		$(obj).parent().parent().addClass("on").siblings().removeClass("on");
	}
	
	function resizeKopoMain() {
		var screenWidth = document.documentElement.clientWidth || document.body.clientWidth;
		if (screenWidth <= 1280) {
			var www = 2000 / 1280 * screenWidth;
			$("#kopo_wrap .main_bg .bxslider").find("li").css("margin-left", (-www / 2) + "px");
			$("#kopo_wrap .main_bg .bxslider").find("li, img").width(www);
			$("#kopo_wrap .main_bg,#kopo_wrap .main_bg .effect_wrap").height(screenWidth / 1280 * 720);
		} else {
			$("#kopo_wrap .main_bg .bxslider").find("li").css("margin-left", "-1000px");
			$("#kopo_wrap .main_bg .bxslider").find("li, img").width(2000);
			$("#kopo_wrap .main_bg,#kopo_wrap .main_bg .effect_wrap").height(720);
		}
		
	}
	
	function resizeKopoMainQuick() {
		kopoIsM=false;
		var screenWidth = document.documentElement.clientWidth || document.body.clientWidth;
		if (screenWidth <= 753) {
			kopoIsM=true;
			var www = screenWidth - $(".quick_wrap .quick_next_box").width()*2; 
			$(".quick_wrap.kopo .quick_box").width(www);
			var children = $(".quick_wrap.kopo .quick").children();
			if(children.size()>3){
				children.eq(Math.round(children.size() / 2)).css('clear', 'left');
				$(".kopo.quick_wrap .quick_box,.kopo.quick_wrap .quick_next_box,.kopo.quick_wrap .quick_prev_box").height(190);
			}
			children.each(function(i, element) {
				$(this).width(www / 3)
			});
			
		} else if (screenWidth <= 980) {
			$(".quickItem").css('clear', 'none');
			var www = screenWidth - $(".quick_wrap .quick_next_box").width()*2 -$(".quick_wrap .quick_bt >div >span span").width();
			$(".quick_wrap.kopo .quick_box").width(www);
			$(".kopo.quick_wrap .quick_box,.kopo.quick_wrap .quick_next_box,.kopo.quick_wrap .quick_prev_box").height(95);
		} else {
			$(".quickItem").css('clear', 'none');
			$(".quick_wrap.kopo .quick_box").width(770);
			$(".kopo.quick_wrap .quick_box,.kopo.quick_wrap .quick_next_box,.kopo.quick_wrap .quick_prev_box").height(95);
		}
		
		if(kopoIsM){
			if($('.quick_wrap.kopo .quick li').size()<=mKopoQuickMinScr){
				$('.quick_wrap.kopo .quick_next_box a.quick_sbtn_right').hide();
			}
		}else{
			if($('.quick_wrap.kopo .quick li').size()<=kopoQuickMinScr){
				$('.quick_wrap.kopo .quick_next_box a.quick_sbtn_right').hide();
			}
		}
		
	}
	
	var kopoQuickElement;
	var kopokopoQuickElementW = 0;
	
	function kopoQuick() {
		if(kopoquickBt){
			if ($('.quick_wrap.kopo .quick_menu').is(":visible")) {
				$('.quick_bt .quick_bt_box a img').attr("src", "/images/main/quick_arrow2.png");
				$('.quick_wrap.kopo .quick_menu').slideUp(500, function() {});
				$('.quick_wrap.kopo .quick_bt_box_close').slideUp(500, function() {});
			} else {
				$('.quick_bt .quick_bt_box a img').attr("src", "/images/main/quick_arrow.png");
				$('.quick_wrap.kopo .quick_menu').slideDown(500, function() {});
				$('.quick_wrap.kopo .quick_bt_box_close').slideDown(500, function() {});
			}
		}
	}
	var kopoQuickMaxScr=0;
	var kopoQuickNextCnt=0;
	var kopoQuickMinScr=4;
	
	var mKopoQuickMaxScr=0;
	var mKopoQuickMinScr=6;
	var kopoIsM=false;
	function kopoNextQuickScr() {
		kopoQuickNextCnt++;
		if (kopokopoQuickElementW == 0) {
			kopoQuickElement = $('.quick_wrap.kopo .quick li').first();
		} else {
			kopoQuickElement = kopoQuickElement.next();
		}
		kopokopoQuickElementW += kopoQuickElement.width();
		if((kopoIsM ? mKopoQuickMaxScr : kopoQuickMaxScr)<kopoQuickNextCnt){
			$('.quick_wrap.kopo .quick_next_box a.quick_sbtn_right').hide();
		}
		$('.quick_box').animate({
			scrollLeft : kopokopoQuickElementW
		}, 1000, function() {
			$('.quick_wrap.kopo .quick_prev_box a.quick_sbtn_left').show();
			
		});
	}
	
	function kopoPrevQuickScr() {
		kopoQuickNextCnt--;
		kopokopoQuickElementW -= kopoQuickElement.width();
		if (kopokopoQuickElementW == 0) {
			$('.quick_wrap.kopo .quick_prev_box a.quick_sbtn_left').hide();
		}
		$('.quick_box').animate({
			scrollLeft: kopokopoQuickElementW
		}, 1000, function () {

			if ((kopoIsM ? mKopoQuickMaxScr : kopoQuickMaxScr) >= kopoQuickNextCnt) {
				$('.quick_wrap.kopo .quick_next_box a.quick_sbtn_right').show();
			}
		});
		kopoQuickElement = kopoQuickElement.prev();
	}
    var kopodelta = 2000;
    var kopoquickBt=true;
    
    function kopoHasScrolled() {
    	var screenWidth = document.documentElement.clientWidth || document.body.clientWidth;
    	if(screenWidth >= 753) {
	    	var screenHeight = document.documentElement.clientHeight || document.body.clientHeight;
	    	var scrollTop = document.documentElement.scrollTop|| document.body.scrollTop;
	    	kopodelta =$('body').prop("scrollHeight")-$('#kopo_footer').outerHeight();
	        var st = scrollTop+screenHeight;
	        if (st > kopodelta){
	            // 퀵 고정
	            $('.quick_wrap.kopo').css("position","relative");
	            $('.quick_wrap.kopo .quick_bt .quick_bt_box_open').hide();
	            $('.quick_wrap.kopo .quick_bt .quick_bt_box_close').show();
	            $('.quick_wrap.kopo .quick_menu').show();
	            $('.quick_wrap.kopo .quick_bt .quick_kopo_move').hide();
	            $('.quick_wrap.kopo .quick_bt .quick_kopo_fix').show();
	            kopoquickBt=false;
	        } else {
	        	// 퀵 움직이도록
	        	$('.quick_wrap.kopo').css("position","fixed")
	        	$('.quick_wrap.kopo .quick_bt .quick_bt_box_open').show();
	            $('.quick_wrap.kopo .quick_bt .quick_kopo_move').show();
	            $('.quick_wrap.kopo .quick_bt .quick_kopo_fix').hide();
	        	kopoquickBt=true;
	        }
    	}
    }	
	

</script>

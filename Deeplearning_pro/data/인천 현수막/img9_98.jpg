<script>
var tlTimer;
var btimer = false;

function camMover(cam) {
	$('.tooltip').hide();
	$(cam).next().show();
	hide_timer (false);
}

function camMout() {
	hide_timer (true);
}

function camToggle(cam) {
	if ($('.tooltip').is(":visible")) {
		$('.tooltip').hide();
	} else { 
		$('.tooltip').hide();
		$(cam).next().show();
	}
}

function hide_timer(btimer) {
	if (btimer) {
		tlTimer = setTimeout(function(){
			$('.tooltip').hide();
		  }, 1500);
	} else {
		clearTimeout(tlTimer);
		tlTimer = null;
	}
}
</script>

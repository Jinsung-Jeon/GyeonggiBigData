    $(document).ready(function(){
        ll_campusl02Slider= $('.ll_campus.news_ad_content').bxSlider({
            autoStart : true,
            enablePager:false,
            auto: true,
			infiniteLoop: true,
            controls:false,
            pagerCustom : '.ll_campus.l02_pager',
            onPagerCallback : function (val){
            	$(".ll_campus.l02_pager .slider-dot").eq(val.current).addClass("over").siblings(".slider-dot").removeClass("over");
            	if(val.total ==1){
            		$(".l02_pager_ctrl.ll_campus").hide();
                }else{
                	$(".l02_pager_ctrl.ll_campus").show();
                }
                setSliderTitle(val.current, "ll_campus");
        	},
            onSliderLoad : setSliderTitle
        });
        $(".ll_campus.l02_slider.ctrl-btn").on("click",function(event){
            event.preventDefault();
            if($(this).hasClass("btn-stop")){
                ll_campusl02Slider.stopAuto();
                $(this).hide();
                $(".ll_campus.l02_slider.btn-play").show();
            }else{
				setSliderReload(true);
                ll_campusl02Slider.startAuto();
                $(this).hide();
                $(".ll_campus.l02_slider.btn-stop").show();

            }
        });
		$(".ll_campus.ctrl-btn.l02_slider").on('keydown', function(e){		
			if(e.which == 9 && !e.shiftKey ){ // 탭키이동시
				//$(".ll_campus.ctrl-btn.l02_slider").trigger('click');
                ll_campusl02Slider.stopAuto();
                $(".ll_campus.l02_slider.btn-stop").hide();
                $(".ll_campus.l02_slider.btn-play").show();
				setSliderReload(false);
				//setTimeout(function(){$('ul.ll_campus.ftban_img_l02 > li:first-child > a').focus();}, 200);//IE이슈
			}
		});
		$('.news_ad_content.news_ad_l02.ll_campus > a').on('focusin', function(){
			var idx = $(this).index();
			//console.log("focusin:"+idx);
			setSliderTitle(idx, "ll_campus");
		});
		function setSliderReload(isTrue){
			
			ll_campusl02Slider.reloadSlider({
				auto: isTrue,
				infiniteLoop : isTrue,
				controls:false,
				enablePager:false,
				pagerCustom : '.ll_campus.l02_pager',
				onPagerCallback : function (val){
					$(".ll_campus.l02_pager .slider-dot").eq(val.current).addClass("over").siblings(".slider-dot").removeClass("over");
					if(val.total ==1){
						$(".l02_pager_ctrl.ll_campus").hide();
					}else{
						$(".l02_pager_ctrl.ll_campus").show();
					}
					setSliderTitle(val.current, "ll_campus");
				},
				onSliderLoad : setSliderTitle
			});
		}
    });

    function setSliderTitle(idx, target){
        var alt = $("."+target+".l02_pager .slider-dot").find("span").eq(idx).text();
		if(alt != "") {
			$(".l02_title."+target).text(alt);
		}
    }
</script>


    

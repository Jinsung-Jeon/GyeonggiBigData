<script>
$(document).ready(function() {
		
	
		
		resizeCampusMain();
		ResizeEvents.addListener(resizeCampusMain, true);
		ResizeEvents.addListener(resizeCampusQuick, true);
			

		
		mQuickMaxScr=Math.round($('.quick_wrap.cam .quick li').size()/2)-4
		quickMaxScr=$('.quick_wrap.cam .quick li').size()-5;
		
		
		
		$('.bxslider.rep li,.bxslider.campus li').find('map').find('area').each(function(){
			if($(this).parent().hasClass('is_m')){
				var pairs = $(this).attr("coords").split(', ');
		        for(var i=0; i<pairs.length; i++) {
		        	var nums = pairs[i].split(',');
		        	for(var j=0; j<nums.length; j++) {
		        		if(j==0||j==2){
		               		 nums[j] = parseFloat(nums[j]) -510;
		        		}else{
		        			 nums[j] = parseFloat(nums[j]);
		        		}
		            }
		            pairs[i] = nums.join(',');
		        }
		        $(this).attr("coords", pairs.join(', '));	
			}
	    });
		
	});
	$(window).scroll(function(event){
		hasScrolled();
	});

	
	function resizeCampusMain() {
		var screenWidth = document.documentElement.clientWidth || document.body.clientWidth;
	 	
		$('.bxslider.rep li,.bxslider.campus li').each(function(i,element){
			var i=$(this).find('img');
			
			if(screenWidth > 1280){
				if($(this).find('img').attr('src').indexOf('getImage.do')!=-1){
					i.attr('src',$(this).find('img').attr('src').replace('&view=m',''));
				}else{
					i.attr('src',$(this).find('img').attr('src').replace('980','2000'));
				}
				if(typeof($(this).find('img').attr('usemap'))!="undefined"){
					i.attr('usemap',$(this).find('img').attr('usemap').replace('_m',''));
				}
				$("#wrap .main_bg .bxslider").find("li").css("margin-left", "-1000px");
				$("#wrap .main_bg .bxslider").find("li, img").width(2000);
				$("#wrap .main_bg .effect_wrap,#wrap .main_bg .bx-viewport,#wrap .main_bg .bigBanners").height(720);
				 
			}else if (screenWidth <= 1280 && screenWidth > 980) {
				if($(this).find('img').attr('src').indexOf('getImage.do')!=-1){
					i.attr('src',$(this).find('img').attr('src').replace('&view=m',''));
				}else{
					i.attr('src',$(this).find('img').attr('src').replace('980','2000'));
				}
				if(typeof($(this).find('img').attr('usemap'))!="undefined"){
					i.attr('usemap',$(this).find('img').attr('usemap').replace('_m',''));
				}
				
				var www = 2000 / 1280 * screenWidth;
				$(this).css("margin-left", (-www / 2) + "px");
				$("#wrap .main_bg .bxslider").find("li, img").width(www); 
				
				$("#wrap .main_bg .effect_wrap,#wrap .main_bg .bx-viewport,#wrap .main_bg .bigBanners").height(screenWidth / 1280 * 720);
			}else if (screenWidth <= 980) {
				if($(this).find('img').attr('src').indexOf('getImage.do')!=-1){
					i.attr('src',$(this).find('img').attr('src').replace('&view=m','')+'&view=m');
				}else{
					i.attr('src',$(this).find('img').attr('src').replace('2000','980'));
				}
				if(typeof($(this).find('img').attr('usemap'))!="undefined"){
					i.attr('usemap',$(this).find('img').attr('usemap').replace('_m','')+'_m');
				}
				
				$("#wrap .main_bg .bxslider").find("li").css("margin-left","0px");
				$("#wrap .main_bg .bxslider").find("li, img").width('100%');
				$('#wrap .main_bg .effect_wrap,#wrap .main_bg .bx-viewport,#wrap .main_bg .bigBanners').height(72/98*screenWidth);
			}
		});
		
	}
	function resizeCampusQuick() {
		isM=false;
		var screenWidth = document.documentElement.clientWidth || document.body.clientWidth;
		if (screenWidth <= 753) {
			isM=true;
			var www = screenWidth - $(".quick_wrap .quick_next_box").width()*2; 
			$(".quick_wrap.cam .quick_box").width(www);
			var children = $(".quick_wrap.cam .quick").children();
			if(children.size()>3){
			children.eq(Math.round(children.size() / 2)).css('clear', 'left');
				$(".cam.quick_wrap .quick_box,.cam.quick_wrap .quick_next_box,.cam.quick_wrap .quick_prev_box").height(190);
			}
			
			children.each(function(i, element) {
				$(this).width(www / 3)
			});
		} else if (screenWidth <= 980) {
			$(".quickItem").css('clear', 'none');
			var www = screenWidth - $(".quick_wrap .quick_next_box").width()*2 -$(".quick_wrap .quick_bt >div >span span").width();
			$(".quick_wrap.cam .quick_box").width(www);
			$(".cam.quick_wrap .quick_box,.cam.quick_wrap .quick_next_box,.cam.quick_wrap .quick_prev_box").height(95);
		} else {
			$(".quickItem").css('clear', 'none');
			$(".quick_wrap.cam .quick_box").width(770);
			$(".cam.quick_wrap .quick_box,.cam.quick_wrap .quick_next_box,.cam.quick_wrap .quick_prev_box").height(95);
		}
		
		if(isM){
			if($('.quick_wrap.cam .quick li').size()<=mQuickMinScr){
				$('.quick_wrap.cam .quick_next_box a.quick_sbtn_right').hide();
			}
		}else{
			if($('.quick_wrap.cam .quick li').size()<=quickMinScr){
				$('.quick_wrap.cam .quick_next_box a.quick_sbtn_right').hide();
			}
		}
		
	}
	
	var quickElement;
	var quickElementW = 0;
	
	function quick() {
		if(quickBt){
			if ($('.quick_wrap.cam .quick_menu').is(":visible")) {
				$('.quick_wrap.cam .quick_bt .quick_bt_box a img').attr("src", "/images/main/quick_arrow2.png");
				$('.quick_wrap.cam .quick_menu').slideUp(500);
				$('.quick_wrap.cam .quick_bt_box_close').slideUp(500);
			} else {
				$('.quick_wrap.cam .quick_bt .quick_bt_box a img').attr("src", "/images/main/quick_arrow.png");
				$('.quick_wrap.cam .quick_menu').slideDown(500);
				$('.quick_wrap.cam .quick_bt_box_close').slideDown(500);
			}
		}
	}
	
	var quickMaxScr=0;
	var quickNextCnt=0;
	var quickMinScr=4;
	
	var mQuickMaxScr=0;
	var mQuickMinScr=6;
	var isM=false;
	function nextScroll() {
		quickNextCnt++;
		if (quickElementW == 0) {
			quickElement = $('.quick_wrap.cam .quick li').first();
		} else {
			quickElement = quickElement.next();
		}
		quickElementW += quickElement.width();
		console.log(mQuickMaxScr);
		if((isM ? mQuickMaxScr : quickMaxScr)<quickNextCnt){
			$('.quick_wrap.cam .quick_next_box a.quick_sbtn_right').hide();
		}
		$('.quick_wrap.cam .quick_box').animate({
			scrollLeft : quickElementW
		}, 1000, function() {
			$('.quick_wrap.cam .quick_prev_box a.quick_sbtn_left').show();
			
		});
	}

	function prevScroll() {
		quickNextCnt--;
		quickElementW -= quickElement.width();
		if (quickElementW == 0) {
			$('.quick_wrap.cam .quick_prev_box a.quick_sbtn_left').hide();
		}
		$('.quick_wrap.cam .quick_box').animate({
			scrollLeft : quickElementW
		}, 1000, function() {
			
			if((isM ? mQuickMaxScr : quickMaxScr)>=quickNextCnt){
				$('.quick_wrap.cam .quick_next_box a.quick_sbtn_right').show();
			}
		});
		quickElement = quickElement.prev();
	}
	
    
    var delta = 2000;
    var quickBt=true;
    
    function hasScrolled() {
    	var screenWidth = document.documentElement.clientWidth || document.body.clientWidth;
    	if(screenWidth >= 753) {
	    	var screenHeight = document.documentElement.clientHeight || document.body.clientHeight;
	    	var scrollTop = document.documentElement.scrollTop|| document.body.scrollTop;
	    	delta =$('body').prop("scrollHeight")-$('#kopo_footer').outerHeight();
	        var st = scrollTop+screenHeight;
	        if (st > delta){
	            // 퀵 고정
	            $('.quick_wrap.cam').css("position","relative");
	            $('.quick_wrap.cam .quick_bt .quick_bt_box_open').hide();
	            $('.quick_wrap.cam .quick_bt .quick_bt_box_close').show();
	            $('.quick_wrap.cam .quick_menu').show();
	            $('.quick_wrap.cam .quick_bt .quick_cam_move').hide();
	            $('.quick_wrap.cam .quick_bt .quick_cam_fix').show();
	            quickBt=false;
	        } else {
	        	// 퀵 움직이도록
	        	$('.quick_wrap.cam').css("position","fixed")
	        	$('.quick_wrap.cam .quick_bt .quick_bt_box_open').show();
	            $('.quick_wrap.cam .quick_bt .quick_cam_move').show();
	            $('.quick_wrap.cam .quick_bt .quick_cam_fix').hide();
	        	quickBt=true;
	        }
    	}
    }
    
    function getDay(sDate, eDate) {
        var mm = sDate.substr(5, 2);
        var dd = sDate.substr(8);
        
        if (sDate == eDate) {
            return mm + "-" + dd;
        } else {
            var mm2 = eDate.substr(5, 2);
            var dd2 = eDate.substr(8);
            return mm + "-" + dd + " ~ " + mm2 + "-" + dd2;
        }
    }
    
</script>
